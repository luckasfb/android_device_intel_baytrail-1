<resources>
    <!-- Flag indicating if the tty is enabled -->
    <bool name="tty_enabled">false</bool>

    <!-- Flag indicating if hac is enabled -->
    <bool name="hac_enabled">true</bool>

    <!-- Show enabled lte option for lte device -->
    <bool name="config_enabled_lte" translatable="false">true</bool>

    <!-- Flag indicating the usage of OEM Hooks for telephony -->
    <bool name="config_usage_oem_hooks_supported">true</bool>
    <!-- These two config items are valid only if config_usage_oem_hooks_supported is true -->
    <!-- The oem hook jar file name -->
    <string name="config_oem_hook_jar_file">/system/framework/com.intel.internal.telephony.OemTelephony.jar</string>
    <!-- The oem hook class name -->
    <string name="config_oem_hook_class_name">com.intel.internal.telephony.OemTelephony.OemTelephonyInterfaceCreator</string>
</resources>
